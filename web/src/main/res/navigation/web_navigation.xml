<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/webViewFragment">

    <fragment
        android:id="@+id/webViewFragment"
        android:name="com.ymy.web.custom.ui.WebViewFragment"
        android:label="WebViewFragment"
        tools:layout="@layout/fragment_webview" >
        <argument
            android:name="url"
            android:defaultValue=""
            app:argType="string" />
        <argument
            android:name="showTitleBar"
            app:argType="boolean"
            android:defaultValue="true" />
        <argument
            android:name="title"
            android:defaultValue=""
            app:argType="string" />
        <argument
            android:name="needBackgroundColor"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="showRightMenu"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="showBackIcon"
            android:defaultValue="true"
            app:argType="boolean" />
        <argument
            android:name="addTopPadding"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="showRefreshBar"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="pushData"
            android:defaultValue=""
            app:argType="string" />
    </fragment>

</navigation>